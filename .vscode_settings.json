{
    "terminal.external.osxExec": "Hyper.app",
    "editor.fontSize": 17,
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
    "terminal.explorerKind": "external",
    "terminal.integrated.lineHeight": 1.3,
    "terminal.integrated.rendererType": "canvas",
    "workbench.colorTheme": "Monokai Dimmed",
    "window.newWindowDimensions": "inherit",
    "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
    "workbench.editor.enablePreview": true,
    "editor.renderWhitespace": "all",
    "[javascript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "[python]": {
        "editor.formatOnSave": true
    },
    "yaml.schemas": {
        // Basically telling it to use kubernetes schema for all yaml files in here
        // "https://raw.githubusercontent.com/compose-spec/compose-spec/master/schema/compose-spec.json": "*compose.yaml",
        // "kubernetes": "*.yaml"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "go.toolsManagement.autoUpdate": true,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
}
